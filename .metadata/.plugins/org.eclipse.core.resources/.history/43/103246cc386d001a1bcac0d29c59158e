
public class Application {
	public static void main(String[] args) {
		int myInt = 82;
		String text = "Heyooo";
		String blank = " ";
		String name = "Globergogg";
		String greet = text+blank+name;
		
		System.out.println(greet);
		System.out.println("hello mr."+blank+name+", your number is "+myInt);
	}
}
