class Person {
	String name;
	int age;
}

public class Appli {
	public static void main(String[] args) {
		Person person1= new Person();
		person1.name="Jhonny Blobber";
		person1.age=28;
		
		Person person2 = new Person();
		person2.name="Mary Berry";
		person2.age=19;
		
		System.out.println(person1.name);

	}
}
